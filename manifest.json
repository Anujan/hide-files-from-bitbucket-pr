{
  "name": "Hide Files From Bitbucket PR",
  "description": "Hide irrelevant files from Bitbucket pull requests.",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "icons": {
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://bitbucket.org/*/*/pull-requests/*"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ]
}
